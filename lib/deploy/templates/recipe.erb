require 'deploy'
<% if additional_require %>
require '<%= additional_require %>'
<% end %>

class <%= clazz_name %> < <%= extends_file %>
<% if commons_include %>
  include ::Deploy::Utils::Common
<% end %>

<% prepends.each do |prepend| %>
  prepend :<%= prepend %>
<% end %>

<% appends.each do |append| %>
  append :<%= append %>
<% end %>

<% method_names.each do |method_name| %>
  desc '<%= method_name %>', "" do
  end

<% end %>
end